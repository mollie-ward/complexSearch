{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": ["http://localhost:3000"]
  },
  "RateLimiting": {
    "RequestsPerMinute": 10,
    "RequestsPerHour": 100
  },
  "Search": {
    "MaxResults": 100,
    "DefaultResults": 10
  },
  "AzureAISearch": {
    "Endpoint": "",
    "ApiKey": "",
    "IndexName": "vehicles-index",
    "VectorDimensions": 1536
  },
  "AzureOpenAI": {
    "Endpoint": "",
    "ApiKey": "",
    "EmbeddingDeploymentName": "text-embedding-ada-002",
    "ChatDeploymentName": "gpt-4",
    "MaxConcurrentRequests": 5,
    "MaxRetries": 3,
    "BatchSize": 100
  },
  "QualitativeTerms": {
    "Terms": {
      "affordable": [
        {
          "FieldName": "price",
          "Operator": "LessThanOrEqual",
          "Value": 15000
        }
      ],
      "cheap": [
        {
          "FieldName": "price",
          "Operator": "LessThanOrEqual",
          "Value": 12000
        }
      ],
      "expensive": [
        {
          "FieldName": "price",
          "Operator": "GreaterThanOrEqual",
          "Value": 30000
        }
      ],
      "economical": [
        {
          "FieldName": "engineSize",
          "Operator": "LessThanOrEqual",
          "Value": 2.0
        },
        {
          "FieldName": "fuelType",
          "Operator": "In",
          "Value": ["Electric", "Hybrid"]
        }
      ],
      "reliable": [
        {
          "FieldName": "mileage",
          "Operator": "LessThanOrEqual",
          "Value": 60000
        }
      ],
      "low mileage": [
        {
          "FieldName": "mileage",
          "Operator": "LessThanOrEqual",
          "Value": 30000
        }
      ],
      "high mileage": [
        {
          "FieldName": "mileage",
          "Operator": "GreaterThanOrEqual",
          "Value": 100000
        }
      ],
      "family car": [
        {
          "FieldName": "numberOfDoors",
          "Operator": "GreaterThanOrEqual",
          "Value": 5
        },
        {
          "FieldName": "bodyType",
          "Operator": "In",
          "Value": ["SUV", "MPV", "Estate", "Hatchback"]
        }
      ]
    }
  },
  "ConversationSession": {
    "StorageType": "InMemory",
    "SessionTimeoutHours": 4,
    "MaxMessagesPerSession": 100,
    "CleanupIntervalHours": 1
  }
}
